@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Animais</h1>
    <p>
        Cadastro de  animais em memória
    </p>
    <p><a class="btn btn-default" href="/animais">Clique aqui para cadastrar</a></p>
</div>
<hr />
<table class="table">
    <thead>
        <tr>
            <th>id</th>
            <th>Nome</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var animal in Model)
        {
            <tr>
                <td>-</td>
                <td>@animal.Nome</td>
            </tr>
        }
    </tbody>
</table>
